mainmenu "Linux/$ARCH $KERNELVERSION Kernel Configuration"

config SRCARCH
	string
	option env="SRCARCH"

source "arch/$SRCARCH/Kconfig"
config PLATFORM_UBUNTU
	tristate "ubuntu: wifi uses only one interface, bt uses bluez"
